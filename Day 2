def find_missing_number(arr):
    n = len(arr) + 1  
    expected_sum = n * (n + 1) // 2
    actual_sum = sum(arr)
    return expected_sum - actual_sum

print("Missing number:", find_missing_number([1, 2, 4, 5]))          
print("Missing number:", find_missing_number([2, 3, 4, 5]))         
print("Missing number:", find_missing_number([1, 2, 3, 4]))          
print("Missing number:", find_missing_number([1]))                   
print("Missing number:", find_missing_number(list(range(1, 6))))  
