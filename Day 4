def merge(arr1, arr2):
    m, n = len(arr1), len(arr2)

    for i in range(m):
  
          if arr1[i] > arr2[0]:
            
            arr1[i], arr2[0] = arr2[0], arr1[i]
            
            arr2.sort()

arr1, arr2 = [1, 3, 5], [2, 4, 6]
merge(arr1, arr2)
print(arr1, arr2)  

arr1, arr2 = [10, 12, 14], [1, 3, 5]
merge(arr1, arr2)
print(arr1, arr2)  

arr1, arr2 = [2, 3, 8], [4, 6, 10]
merge(arr1, arr2)
print(arr1, arr2)  

arr1, arr2 = [1], [1]
merge(arr1, arr2)
print(arr1, arr2)  
